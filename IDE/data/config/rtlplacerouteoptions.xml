<?xml version="1.0" encoding="UTF-8"?>
<OptionDef>
    <SingleChoiceOption id="PNR01" label="Generate SDF File" default="1">
        <Description>Generate standard delay format file. Default: *.sdf</Description>
        <Choice label="True" cmd="-sdf"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR06" label="Generate Constraint File of Ports" default="1">
        <Description>Generate physical constraint file of ports. Default: *.io.cst</Description>
        <Choice label="True" cmd="-oc"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR09" label="Generate IBIS File" default="1">
        <Description>Generate IBIS（Input/Output Buffer Informational Specifation）file. Default: *.ibs</Description>
        <Choice label="True" cmd="-ibs"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR10" label="Generate Post-Place File" default="1">
        <Description>Generate Post-Place File. Default: *.posp</Description>
        <Choice label="True" cmd="-posp"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR07" label="Generate Post-PNR Simulation Model File" default="1">
        <Description>Generate physical constraint file of ports. Default: *.vo</Description>
        <Choice label="True" cmd="-o"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR03" label="Initialize Primitives" default="1">
        <Description>Add default initial values to instance in the generated post-PNR simulation model file.</Description>
        <Choice label="True" cmd="-init_all"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <!--<SingleChoiceOption id="PNR08" label="Print BSRAM Initial Value" default="0">
        <Description>Print BSRAM initial value to bitstream</Description>
        <Choice label="True"/>
        <Choice label="False" cmd="-bsram_init_np"/>
    </SingleChoiceOption>-->
    <SingleChoiceOption id="PNR02" label="Show All Warnings" default="1">
        <Description>Show all warning messages in running.</Description>
        <Choice label="True" cmd="-warning_all"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <!--<SingleChoiceOption id="PNR04" label="Ignore Parser Error" default="1">
        <Description>Ignore errors in Parser.</Description>
        <Choice label="True" cmd="-T1"/>
        <Choice label="False"/>
    </SingleChoiceOption>-->
    <!--<SingleChoiceOption id="PNR05" label="Ignore Route Failure" default="1">
        <Description>Ignore all errors in Placement and Router.</Description>
        <Choice label="True" cmd="-T2"/>
        <Choice label="False"/>
    </SingleChoiceOption>-->
    <SingleChoiceOption id="PNR11" label="Generate Plain Text Timing Report" default="1">
        <Description>Generate Plain Text Timing Report</Description>
        <Choice label="True" cmd="-tt"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR12" label="Run Timing Driven" default="0">
        <Description>Run Timing Driven</Description>
        <Choice label="True" cmd="-timing"/>
        <Choice label="False"/>
    </SingleChoiceOption>
<!--    <SingleChoiceOption id="PNR13" label="Place Absoluate/Relative Locations" default="0">
        <Description>True: Place module as absoluate locations according to firm core file.&#x000A;False: Place module as relative locations according to firm core file.</Description>
        <Choice label="True" cmd="-abs_loc"/>
        <Choice label="False"/>
    </SingleChoiceOption>-->
    <SingleChoiceOption id="PNR14" label="Use SCF" default="1">
        <Description>Use the .scf file which is generated by Synplify_pro as an additional timing contraints file.</Description>
        <Choice label="True" cmd="-scf"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR15" label="Not place register to IOB" default="1">
        <Description>Do not place register to IOB in implement.</Description>
        <Choice label="True" cmd="-reg_not_in_iob"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR16" label="Promote Physical Constraint Warning to Error" default="1">
        <Description>Promote Physical Constraint Warning to Error.</Description>
        <Choice label="True" cmd="-cst_error"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR17" label="Report Auto-Placed IO Information" default="1">
        <Description>Report auto-placed io information in placement.</Description>
        <Choice label="True" cmd="-no_cst_warning"/>
        <Choice label="False"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR18" label="Place Option" default="1">
        <Description></Description>
        <Choice label="1" cmd="-place_option 1"/>
        <Choice label="0"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="PNR19" label="Route Option" default="1">
        <Description></Description>
        <Choice label="1" cmd="-route_option 1"/>
        <Choice label="0"/>
    </SingleChoiceOption>
</OptionDef>
