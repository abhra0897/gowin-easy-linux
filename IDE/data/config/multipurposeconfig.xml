    <SingleChoiceOption id="MP06" label="Use DONE as Regular IO" default="0">
        <Description></Description>
        <Choice label="True" cmd="set DONE regular_io = true"/>
        <Choice label="False" cmd="set DONE regular_io = false"/>
    </SingleChoiceOption>
    <SingleChoiceOption id="MP07" label="Use RECONFIG_N as Regular IO" default="0">
        <Description></Description>
        <Choice label="True" cmd="set RECONFIG_N regular_io = true"/>
        <Choice label="False" cmd="set RECONFIG_N regular_io = false"/>
    </SingleChoiceOption>
</OptionDef>
