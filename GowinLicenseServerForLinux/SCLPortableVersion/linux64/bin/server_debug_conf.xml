<?xml version="1.0"?>
<CUSTOMLOG version="1.0">
	<LogFileProperty>
		<!-- Name of the log file.  Please Make sure that user 
		     starting Licensing server has write permission on 
		     this file and directory containing this file. 
		     Default value is snpslmd.log.
		 --> 
		<fileName>snpslmd.log</fileName>
		<!-- Delimiter between each Property Value.  --> 
		<delimiter>\t</delimiter>
		<!-- Custome Log Rotation Type.
		  Valid Values 
			(a) size ( Log Rotation Based of File Size, default value is 100 MB).
			(b) date ( Log Rotation Based on Date ). 
			(c) None ( Disable Log Rotation).
		-->
		<rotate_policy>size</rotate_policy>
		<!-- Value in MB. Please note maxLogSize can only be interger.  -->
		<maxLogSize>100</maxLogSize>
		<!-- Value which will prefix before each ativity entry in Log --> 
		<prefix>(snpslmd)</prefix>
	</LogFileProperty>
	
	<!-- Type  of Command i.e. OUT,IN -->
	<Property value="status">
		<prefix>Command</prefix>
		<prolog>(</prolog>
		<epilog>)</epilog>
