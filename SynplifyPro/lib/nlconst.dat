CONS{create_clock
	PARAM{-disable				TYPE{FLAG}}
	PARAM{-syn_tdm				TYPE{STRING} MSG{INTERNAL}} 
 	PARAM{-syn_fa_disable			TYPE{STRING} MSG{INTERNAL}}
 	PARAM{-haps_constraint			TYPE{STRING} MSG{INTERNAL}}
	PARAM{-internal				TYPE{INT}}
	PARAM{-comment				TYPE{STRING}}
	PARAM{-proxy_name			TYPE{STRING}}
	PARAM{-proxy_invert			TYPE{FLAG}}
	PARAM{-add				TYPE{FLAG}}
	PARAM{-force_sync_all_clocks		TYPE{FLAG}}
	PARAM{-name				TYPE{STRING}}
	PARAM{-period				TYPE{FLOAT}			COND{PAR>=0}}
	PARAM{-waveform				TYPE{LIST}			COND{LEN==2 && PAR0>=0 && PAR0<PAR1}}
	PARAM{-priority				TYPE{INT} MSG{INTERNAL}}
	PARAM{-freq				TYPE{FLOAT}			COND{PAR>=0}}
	PARAM{object				TYPE{LIST}		TARG{PORT INST PIN NET}}
	COND{PAR{-period} PAR{object}}
	COND{PAR{-period}}
	CONDLE{PAR{-waveform} PAR{-period}}
	CONDPAIR{PAR{-name} PAR{-proxy_name}}
	CONDPAIR{PAR{-proxy_name} PAR{-proxy_invert}}
	TYPE{dc}
}
CONS{create_generated_clock
	PARAM{-quiet				TYPE{FLAG}}
	PARAM{-disable				TYPE{FLAG}}
	PARAM{-syn_tdm				TYPE{STRING} MSG{INTERNAL}} 
 	PARAM{-syn_fa_disable		TYPE{STRING} MSG{INTERNAL}}
 	PARAM{-haps_constraint		TYPE{STRING} MSG{INTERNAL}}
	PARAM{-comment				TYPE{STRING}}
	PARAM{-add					TYPE{FLAG}}
	PARAM{-name					TYPE{STRING}}
	PARAM{-proxy_name			TYPE{STRING}}
	PARAM{-proxy_invert			TYPE{FLAG}}
	PARAM{-source				TYPE{LIST}		COND{LEN==1} TARG{PORT PIN NET}}
	IFXDC{
		PARAM{-edges			TYPE{LIST_INT}		COND{LEN==3 && PAR0>0 && PAR1>PAR0 && PAR2>PAR1} MSG{101}}
	} 
	IFNXDC{
		PARAM{-edges			TYPE{LIST_INT}		COND{LEN==3 && PAR0>0 && PAR1>=PAR0 && PAR2>=PAR1} MSG{101}}
	}
	PARAM{-edge_shift			TYPE{LIST_FLOAT}	COND{LEN==3}}
	PARAM{-divide_by			TYPE{INT}			COND{PAR>0} MSG{103}}
	PARAM{-multiply_by			TYPE{INT}			COND{PAR>0}}
	PARAM{-duty_cycle			TYPE{FLOAT}			COND{PAR>0 && PAR<100}  }
	PARAM{-invert				TYPE{FLAG}}
	PARAM{-master_clock			TYPE{LIST} COND{LEN==1} TARG{CLOCK}}
	PARAM{-preinvert			TYPE{FLAG}}
	PARAM{-combinational			TYPE{FLAG}}
	PARAM{-phase				TYPE{FLOAT}}
	PARAM{-offset				TYPE{FLOAT}}
	PARAM{-priority				TYPE{INT} MSG{INTERNAL}}
	PARAM{-force_sync_all_clocks		TYPE{FLAG}}
	PARAM{object				TYPE{LIST}			TARG{PORT INST PIN NET}}
	CONDEXCL{PAR{-edges} PAR{-divide_by}}
	CONDEXCL{PAR{-edges} PAR{-multiply_by}}
	CONDPAIR{PAR{-edges} PAR{-edge_shift}}
	CONDPAIR{PAR{-source} PAR{-edges}}
	CONDPAIR{PAR{-source} PAR{-edge_shift}}
	CONDPAIR{PAR{-source} PAR{-divide_by}}
	CONDPAIR{PAR{-source} PAR{-multiply_by}}
	CONDPAIR{PAR{-source} PAR{-invert}}
	CONDPAIR{PAR{-source} PAR{-preinvert}}
	CONDPAIR{PAR{-source} PAR{-combinational}}
	CONDPAIR{PAR{-source} PAR{-phase}}
	CONDPAIR{PAR{-source} PAR{-offset}}
	IFXDC{
		CONDPAIR{PAR{-master_clock} PAR{-add}}
	}
	CONDPAIR{PAR{-name} PAR{-proxy_name}}
	CONDPAIR{PAR{-proxy_name} PAR{-proxy_invert}}
	TYPE{dc}
}

